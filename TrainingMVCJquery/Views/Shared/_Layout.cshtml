    @*script to change active class of sidebar link*@
    <script>
        $(document).ready(function() {
            // remove all active class on reload
            $(".sidebar-wrapper ul li").removeClass("active");

            // get stored title and index
            var activeIndex = localStorage.getItem("activeIndex");
            var title = localStorage.getItem("pageTitle");

            // add active class to the li with index and set title
            $('#nav-list').find('li:eq(' + activeIndex + ')').addClass("active");
            $("title").text(title);

            $(".sidebar-wrapper ul li").on("click", function() {
                var newIndex = $(this).index(); // get index of clicked li
                var newTitle = $(this).find('p').text();

                // set new values in local storage
                localStorage.setItem("activeIndex", newIndex);
                localStorage.setItem("pageTitle", newTitle);
            });
        })
    </script>
